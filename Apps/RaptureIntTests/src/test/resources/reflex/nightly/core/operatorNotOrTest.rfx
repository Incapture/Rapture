println('Compare NOT and OR');

resultsNOTOR=[];


for i in bools do
	for j in bools do
	
		if (!i||j)==((!i)||j) do
			resultsNOTOR = resultsNOTOR + true;
		else do
			resultsNOTOR = resultsNOTOR + false;
			println('failed for (!i||j)==((!i)||j)');
		end
	
		
		if (!i||!j)==((!i)||(!j)) do
			resultsNOTOR = resultsNOTOR + true;
		else do
			resultsNOTOR = resultsNOTOR + false;
			println('failed for (!i||!j)==((!i)||(!j))');
		end
		
		
		if (i||!j)==((i)||(!j)) do
			resultsNOTOR = resultsNOTOR + true;
		else do
			resultsNOTOR = resultsNOTOR + false;
			println('failed for (i||!j)==((i)||(!j))');
		end
	end
end


