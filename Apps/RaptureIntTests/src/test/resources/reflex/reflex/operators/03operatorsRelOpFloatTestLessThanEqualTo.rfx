returnVal = false;

//---------------test-----------------
//--------relational operators--------
//---Floats, positive and negative.---

//----------less than/equal to---------
println("\nTest less than/equal to operator.");
answerLess = [];
resultLess = [];

for i = -50 to 42 do 
	x=i*0.1;
	answerLess = answerLess + x;
end

for j = -50 to 50 do
	r=j*0.1;
	if r <= 4.2 do
		resultLess = resultLess +j;
	end
end


println('resultLess: ' + resultLess);
println('answerLess: ' + answerLess);

for k = 0 to 50 do
    if (resultLess[k] <= answerLess[k]) do
        returnVal = true;
    else do
        returnVal = false;
        return returnVal;
    end
end

for l = 51 to 92 do
    if (answerLess[l] <= resultLess[l]) do
        returnVal = true;
    else do
        returnVal = false;
        return returnVal;
    end
end


return returnVal;
