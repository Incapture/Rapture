// test we can catch an exception thrown from a reflex "library" script
//************
require "reflexTest.scriptstore/commonFuncs.rfx" as comm;

// set the return value here of false
    retVal = false;


try do
    comm.throwExceptionTest();
end
catch e do
	println('caught exception: ' + e);
	retVal = true;
end

return retVal;


