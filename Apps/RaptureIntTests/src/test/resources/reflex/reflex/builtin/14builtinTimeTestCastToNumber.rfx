returnVal = false;

//---------------test-----------------


println("\nTesting of cast to number.1");
ntime=time('10:10:10');
println('ntime is '+ ntime);
bnum=cast(ntime, 'number');
println('ntime cast to a number is: '+ bnum);
eval= ((10*60*60)+  (10*60) + 10)*1000;
println('((10*60*60)+  (10*60) + 10)*1000 = '+eval);
diff= bnum-eval;
println('subtracting these two gives: '+ diff);


println("\nTesting of cast to number.2");
nntime=time('5:5:5');
println('nntime is '+ nntime);
bbnum=cast(nntime, 'number');
println('nntime cast to a number is: '+ bbnum);
eeval= ((5*60*60)+  (5*60) + 5)*1000;
println('((5*60*60)+  (5*60) + 5)*1000 = '+eeval);
ddiff= bbnum-eeval;
println('subtracting these two gives: '+ ddiff);

println("\nError of: " + diff/60000 + ' secs i.e ' + (diff/60000)/60 + 'mins');

if  diff == 0 && ddiff == 0  do
	returnVal = true;
else do
	returnVal = false;
end

return returnVal;
