// Date time

println("Now is " + date() + ", time is " + time());

a=date('19990127'); 
b=date('19990128'); 
c=date('19990128'); 
d=date('19990129'); 

assert (b == c);
assert (b <= c);
assert (b >= c);

assert (b != a);
assert (a < b);
assert (b > a);

assert (b != d);
assert (d > b);
assert (b < d);

w=time('00:01:02'); 
x=time('12:34:56'); 
y=time('12:34:56'); 
z=time('23:59:59'); 

assert (x == y);
assert (x <= y);
assert (x >= y);

assert (x != w);
assert (w < x);
assert (x > w);

assert (x != z);
assert (z > x);
assert (x < z);

return true;
