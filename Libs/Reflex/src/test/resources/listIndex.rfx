// Tests of different ways of indexing a string or list

st = "01234567890";

x = st[0 .. 4];
// Above is 0123
// i.e. start from the 0 position, and return items to the 4th (but not inclusive, so the size
//is end - start

println(x);

println(st[4 .. size(st) ]);

a = [ 0, 1, 2, 3, 4];

x = a[ 1 .. 3 ];
assert(x == [ 1, 2 ]);
println(x);

println(a[0 .. 1]);
println(a[0 .. 1]);
println(a[1 .. 2]);
println(a[0 .. size(a)]);


